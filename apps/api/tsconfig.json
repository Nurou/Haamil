{
  "extends": "../../tsconfig.test.json",
  "compilerOptions": {
    "lib": ["ES2023", "DOM"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": "./",
    "paths": {
      "@/api/*": ["./src/*"]
    },
    "typeRoots": ["./node_modules/@types"],
    "outDir": "./dist",
    "rootDir": "./src",
    "noEmit": false,
    "allowJs": true,
    "sourceMap": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
